    "axios": "^0.21.1",
    "dotenv": "^6.0.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.16.0",
    "@types/aws-lambda": "^8.10.64",
    "@types/node": "^14.14.6",
    "aws-sdk": "^2.203.0",
    "aws-sdk-mock": "^1.7.0",
    "chai": "^4.1.2",
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.20",
    "mocha": "^5.0.1",
    "serverless": "^2.16.1",
